import javax.util.Scanner;

public class Eggs
{
    public static void main(String[] args)
    {
	int eggs;
	Scanner input = new Scanner();
	System.out.println("Enter the number of single eggs: ");
	eggs = input.nextInt();
	dozenCount(eggs);	
    }

    public static void dozenCount(int eggs)
    {
	int temp;
	temp = eggs % 12;
    }
}
